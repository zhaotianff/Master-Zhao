                <!--修改系统文件夹路径 我的文档 我的图片 ...-->
                <!--我的电脑添加快速启用项-->
                <!--启用传统图片查看器-->
                <!--OEM信息设置-->
                <!--启动logo-->
                <!--禁止系统记录使用历史记录-->
                <!--右键菜单管理-->
                <!--硬件信息修改-->
                <!--桌面显示Windows版本信息-->
                <!--任务栏缩略图窗口大小 HKEY_CURRENT_USER\Software\Microsoft\Windows\CurrentVersion\Explorer\Advanced-->
                <!--在桌面创建上帝模式文件夹-->
            </GroupBox.Header>
        </GroupBox>
    </StackPanel>
</Page>
